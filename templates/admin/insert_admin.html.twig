{% extends 'admin/home.html.twig' %}

{% block main %}

    <div class="content">
    <div class="form-group">
    {#        {{ form(form) }}#}
    {{ form_start(form) }}

    {{ form_label(form.email, 'Adresse E-mail',{'label_attr':{'class': 'form-label' }}) }}
    {{ form_widget(form.email, {'attr': {'class': 'form-control'}}) }}
    {{ form_errors(form.email) }}

    {{ form_label(form.password, 'Mot de passe',{'label_attr':{'class': 'form-label'}}) }}
    {{ form_widget(form.password, {'attr':{'class': 'form-control'}}) }}
    {{ form_errors(form.password) }}

    {{ form_widget(form.valider, {'attr':{'class': 'btn btn-primary'}}) }}
    {{ form_end(form) }}

{% endblock %}